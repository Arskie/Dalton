Imports encapsulatedserialkey.clsSerialize

Module security

    Friend Function HashMe(ByVal src As String) As String
        Return HashMe(src)
    End Function

    Friend Function Encrypt(ByVal src As String) As String
        Return Encrypt(src)
    End Function

    Friend Function Decrypt(ByVal src As String) As String
        Return Decrypt(src)
    End Function

End Module
